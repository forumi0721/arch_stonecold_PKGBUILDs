pkgname=("systemd-dummy")
pkgver=0.4
pkgrel=1
pkgdesc="systemd-dummy"
arch=("any")
url="https://stonecold.kr/"
license=("custom")
provides=("systemd")
conflicts=("systemd")
replaces=("systemd")
source=(
	"systemd-sysusers"
	"systemd-tmpfiles"
)
md5sums=(
	"SKIP"
	"SKIP"
)

package() {
	install -dm755 "${pkgdir}/usr/bin/"
	install -Dm755 "${srcdir}/systemd-sysusers" "${pkgdir}/usr/bin/"
	install -Dm755 "${srcdir}/systemd-tmpfiles" "${pkgdir}/usr/bin/"
}

